#include <stdio.h>
#include <stdlib.h>

int main(void) {
	printf("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	char *buff = malloc(10);
	scanf("[^\n]", buff);
	printf("%s", buff);
	return EXIT_SUCCESS;
}
